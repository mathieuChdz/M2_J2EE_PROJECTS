<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="adresse" class="fr.amis.tp4.entity.Adresse">
        <property name="rue"   value="Allee des boss"/>
        <property name="ville"   value="Draveil"/>
    </bean>

    <bean id="reference" class="fr.amis.tp4.entity.Reference">
        <property name="siret"   value="765444"/>
    </bean>

    <bean id="fournisseur" class="fr.amis.tp4.entity.Fournisseur" autowire="constructor">
        <property name="nom"   value="michelin"/>
    </bean>

    <bean id="id" class="fr.amis.tp4.entity.Identite" autowire="byType"/>

    <bean id="application" class="fr.amis.tp4.entity.Application" autowire="byName"/>

</beans>
